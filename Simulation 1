GNU nano 6.2                                                                   program1.TCL *                                                                          
set ns [new Simulator]

$ns color 0 Blue
set nf [open program1.nam w]
$ns namtrace-all $nf
set nd [open program1.tr w]
$ns trace-all $nd

proc finish { } {
global ns nf nd
$ns flush-trace
close Snf
close Snd
exec nam prog 1.nam &
exit 0
}

set nO [$ns node]
set n1 [Sns nodel
set n2 [Sns nodel

$ns duplex-link $nO $n1 1Mb 10ms DropTail
$ns duplex-link $n1 $n2 512Kb 10ms DropTail
$ns queue-limit $n1 $n2 5

set udp0 [new Agent/UDP]
$ns attach-agent $nO $udp0
set cbrO [new Application/Traffic/CBR]
$cbrO set packetSize 500
$cbrO set interval 0.005
$cbr0 attach-agent $udp0

set sink [new Agent/Null]
$ns attach-agent $n2 $sink
$ns connect $udp0 $sink
